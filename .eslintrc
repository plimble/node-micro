{ "extends": "eslint-config-airbnb",
  "env": {
    "browser": false,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "modules": true,
    "generators": true
  },
  "rules": {
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-alert": 0,
    "no-script-url": 0,
    "id-length": 0,
    "func-names": 0,
    "import/export": 0,
    "new-cap": 0,
    "default-case": 0,
    "no-unused-expressions": 0,
    "no-new": 0
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": ["node_modules"]
    }
  },
  "globals": {
  }
}
